local script = Instance.new('Script', modong)
	local modong = script.Parent
	local khungyeucau =  script.Parent.Parent.khungyeucau
	modong.MouseButton1Click:Connect(function()
	
		if khungyeucau.Visible == false then
			khungyeucau.Visible = true
			modong.Text = "đóng"
	else
			khungyeucau.Visible = false
			modong.Text = "mở"
		end
	end)

	local script = Instance.new('Script', yeucau1)
	local yeucau1 = script.Parent
	local khungdon1 = yeucau1.Parent.Parent.Parent.khungdon1
	yeucau1.MouseButton1Click:Connect(function()
		khungdon1.Visible = not khungdon1.Visible
	end)


	local script = Instance.new('Script', yeucau2)
	local yeucau2 = script.Parent
	local khungdon2 = yeucau1.Parent.Parent.Parent.khungdon2
	yeucau2.MouseButton1Click:Connect(function()
		khungdon2.Visible = not khungdon2.Visible
	end)


local script = Instance.new('Script', yeucau3)

local yeucau3 = script.Parent
local khungdon3 = script.Parent.Parent.Parent.Parent.khungdon3
yeucau3.MouseButton1Click:Connect(function()
	khungdon3.Visible = not khungdon3.Visible
end)

 
	local script = Instance.new('Script', yeucau4)
	local yeucau4 = script.Parent
	local khungdon4 = yeucau1.Parent.Parent.Parent.khungdon4
