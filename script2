	end)


	local script = Instance.new('Script', playername)
	local playername = script.Parent
	playername.Text = game.Players.LocalPlayer.Name
	local script = Instance.new('Script', xongdon)
	local xongdon = script.Parent
	xongdon.MouseButton1Click:Connect(function()
		if xongdon.Text == "chưa xong" then
			xongdon.Text = "thành công"
		else
			xongdon.Text = "chưa xong"
		end
	end)


	local script = Instance.new('Script', playername_2)
	local playername = script.Parent
	playername.Text = game.Players.LocalPlayer.Name
	
	local script = Instance.new('Script', xongdon_2)
	local xongdon = script.Parent
	xongdon.MouseButton1Click:Connect(function()
		if xongdon.Text == "chưa xong" then
			xongdon.Text = "thành công"
		else
			xongdon.Text = "chưa xong"
		end
	end)
	
 
